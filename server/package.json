{
  "name": "lobaiseo-backend",
  "version": "1.0.0",
  "description": "Backend server for Google Business Profile integration",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "set RUN_MODE=LOCAL && nodemon server.js",
    "dev:azure": "set RUN_MODE=AZURE && nodemon server.js",
    "start:local": "set RUN_MODE=LOCAL && node server.js",
    "start:azure": "set RUN_MODE=AZURE && node server.js",
    "start:production": "node start-production.js",
    "docker:build": "docker build -t lobaiseo-backend:latest .",
    "docker:run": "docker-compose up -d",
    "docker:run:prod": "docker-compose -f docker-compose.production.yml up -d --build",
    "docker:stop": "docker-compose down",
    "docker:stop:prod": "docker-compose -f docker-compose.production.yml down",
    "docker:logs": "docker-compose logs -f backend",
    "docker:logs:prod": "docker-compose -f docker-compose.production.yml logs -f backend",
    "docker:push": "./docker-push.sh scale112",
    "docker:tag": "docker tag lobaiseo-backend:latest scale112/lobaiseo-backend:latest",
    "docker:pull": "docker pull scale112/lobaiseo-backend:v6",
    "docker:run:hub": "docker-compose -f docker-compose.hub.yml up -d",
    "docker:stop:hub": "docker-compose -f docker-compose.hub.yml down"
  },
  "keywords": [
    "google-business-profile",
    "oauth",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^13.5.0",
    "form-data": "^4.0.4",
    "googleapis": "^128.0.0",
    "mailgun.js": "^12.0.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "node-schedule": "^2.1.1",
    "nodemailer": "^7.0.6",
    "qrcode": "^1.5.3",
    "razorpay": "^2.9.6",
    "twilio": "^5.10.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
